{
  "name": "slim/slim-annotation-router",
  "description": "Slim Annotation Router for Slim 4",
  "keywords": ["library", "slim", "bridge", "annotation", "rest", "router", "psr7"],
  "homepage": "https://github.com/nicc0/slim-annotation-router",
  "type": "library",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Tecza",
      "email": "kontakt@danieltecza.pl",
      "homepage": "https://www.danieltecza.pl/"
    }
  ],
  "require": {
    "php": "^7.1",
    "slim/slim": "4.x-dev",
    "slim/psr7": "dev-master",
    "slim/http": "dev-master",
    "doctrine/annotations": "v1.6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "autoload": {
    "psr-4": {
      "Slim\\AnnotationRouter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Slim\\AnnotationRouter\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "php vendor/bin/phpunit"
  }
}